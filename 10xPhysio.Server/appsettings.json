{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Supabase": {
    "Url": "https://your-project.supabase.co",
    "AnonKey": "change-me-anon-key",
    "JwtSecret": "change-me-jwt-secret"
  },
  "AiGeneration": {
    "ApiBaseUrl": "https://openrouter.ai/api/v1/",
    "ApiKey": "change-me-openrouter-api-key",
    "DefaultModel": "openrouter/auto",
    "DefaultTemperature": 0.7,
    "MinTemperature": 0.0,
    "MaxTemperature": 1.2,
    "MinimumContextLength": 120,
    "PromptOverrideLimit": 256,
    "ProviderTimeoutSeconds": 60
  },
  "AllowedHosts": "*"
}
